{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Resources": {
    "MigueletLambda": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "FunctionName": "GetMigueletQuote",
        "Handler": "src/index.js",
        "Role": {
          "Fn::GetAtt": [
            "GetMigueletQuoteRole",
            "Arn"
          ]
        },
        "Code": {
          "S3Bucket": "my-lambda-functions",
          "S3Key": "GetMigueletQuoteCode"
        },
        "Runtime": "nodejs12.x",
        "Timeout": 300
      }
    }
  }
}
